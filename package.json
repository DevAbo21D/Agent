{
  "name": "mabahith-intelligence",
  "description": "Advanced Intelligence Report System",
  "author": "Mabahith Team",
  "repository": {
    "type": "git",
    "url": "https://github.com/example/mabahith-intelligence.git"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "@google/genai": "1.37.0",
    "electron-squirrel-startup": "^1.0.1",
    "framer-motion": "11.11.11",
    "jszip": "^3.10.1",
    "lucide-react": "0.562.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-force-graph-2d": "^1.29.0",
    "recharts": "^3.6.0"
  },
  "build": {
    "appId": "com.mabahith.app",
    "productName": "Mabahith Intelligence",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "afterPack": "scripts/afterPack.cjs",
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "public/favicon.ico"
    },
    "compression": "maximum"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}